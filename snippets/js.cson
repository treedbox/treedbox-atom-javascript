".text.html, .source.js, .source.js-semantic":
  "console.log - JS":
    prefix: "log"
    body: "
    console.log('$1 ',$1);$0
    "
  "addEventListener - JS":
    prefix: "listener"
    body: "
    addEventListener('$1', ${3:on$2${1:ActionName}}, false);$0
    "
#function-------------
  "function - JS":
    prefix: "function"
    body: "
    function ${1:funcName}(){\n
	     \t$2\n
     }$0
    "
  "Anonymous Function - JS":
    prefix: "anonymous"
    body: "
    (function(){\n
      \t$1\n
    })();\n
    $0
    "
  "Anonymous Function Associated - JS":
    prefix: "anonymousa"
    body: "
    var $1 = function(){\n
      \t$2\n
    }();\n
    $0
    "
  "document.write - JS":
    prefix: "write"
    body: "
    document.write(${1markup});$0
    "
  "do...while - JS":
    prefix: "dowhile"
    body: "
    do{\n
      \t$1\n
    z++;\n
    } while(${2:conditional});\n
    $0
    "
#array--------------
