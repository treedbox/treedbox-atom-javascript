  "fetch - JS":
    prefix: "fetch"
    body: "
    fetch('${1:filename.extension}')\n
    	\t.then(function(response){\n
    	   \t\treturn response;\n
    	})\n
    	\t.then(function(data){\n
    	   \t\tconsole.log('data', data);\n
    	})\n
    	\t.catch(function(error){\n
    	   \t\tconsole.log('ERROR: ', error.message);\n
    	})$0
